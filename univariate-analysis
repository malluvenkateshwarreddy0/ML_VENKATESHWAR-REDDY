import pandas as pd

import matplotlib.pyplot as plt

import seaborn as sns

df = sns.load_dataset("titanic")
df.head()

sns.countplot(df['embarked'])

df['sex'].value_counts().plot(kind='pie', autopct='%.2f%%')
plt.show()

sns.boxplot(df['age'])

